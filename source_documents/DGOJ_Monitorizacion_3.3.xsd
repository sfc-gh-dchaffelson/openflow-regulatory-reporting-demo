										<xs:documentation>Fecha y hora final de la ultima partida de ese juego</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Participacion" type="Importe"/>
								<xs:element name="ParticipacionDevolucion" type="Importe"/>
